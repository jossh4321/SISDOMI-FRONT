;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+HA;AACA;AACA","file":"Toolbar.vue","sourceRoot":"src/views/Layout","sourcesContent":["<template>\r\n  <v-app-bar color=\"primary\" fixed dark app>\r\n    <v-toolbar-title class=\"ml-0 pl-3\">\r\n      <v-app-bar-nav-icon @click.stop=\"toogleDrawer\"></v-app-bar-nav-icon>\r\n    </v-toolbar-title>\r\n    <v-text-field\r\n      flat\r\n      solo-inverted\r\n      prepend-icon=\"search\"\r\n      label=\"Search\"\r\n      class=\"hidden-sm-and-down mt-7\"\r\n    >\r\n    </v-text-field>\r\n    <v-spacer></v-spacer>\r\n    <v-btn icon @click=\"handleFullScreen\">\r\n      <v-icon>fullscreen</v-icon>\r\n    </v-btn>\r\n    <v-menu\r\n      offset-y\r\n      origin=\"center center\"\r\n      class=\"elevation-1\"\r\n      :nudge-right=\"140\"\r\n      :nudge-bottom=\"14\"\r\n      transition=\"scale-transition\"\r\n    >\r\n      <template v-slot:activator=\"{ on }\">\r\n        <v-btn icon v-on=\"on\">\r\n          <v-badge color=\"red\" overlap>\r\n            <template v-slot:badge>\r\n              <span class=\"d-block span-alert\">3</span>\r\n            </template>\r\n            <v-icon medium>notifications</v-icon>\r\n          </v-badge>\r\n        </v-btn>\r\n      </template>\r\n      <notification-list></notification-list>\r\n    </v-menu>\r\n    <v-menu\r\n      offset-y\r\n      origin=\"center center\"\r\n      :nudge-right=\"140\"\r\n      :nudge-bottom=\"10\"\r\n      transition=\"scale-transition\"\r\n    >\r\n      <template v-slot:activator=\"{ on }\">\r\n        <v-btn icon large text v-on=\"on\">\r\n          <v-avatar size=\"30\">\r\n            <img src=\"../../assets/static/man_4.jpg\" alt=\"Josue Colombo\" />\r\n          </v-avatar>\r\n        </v-btn>\r\n      </template>\r\n      <v-list class=\"pa-0\">\r\n        <v-list-item\r\n          v-for=\"(item, index) in items\"\r\n          :to=\"!item.href ? { name: item.name } : null\"\r\n          :href=\"item.href\"\r\n          @click=\"item.click\"\r\n          ripple=\"ripple\"\r\n          :disabled=\"item.disabled\"\r\n          :target=\"item.target\"\r\n          ref=\"noopener\"\r\n          :key=\"index\"\r\n        >\r\n          <v-list-item-action v-if=\"item.icon\">\r\n            <v-icon>{{ item.icon }}</v-icon>\r\n          </v-list-item-action>\r\n          <v-list-item-content>\r\n            <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      </v-list>\r\n    </v-menu>\r\n  </v-app-bar>\r\n</template>\r\n\r\n<script>\r\nimport Util from \"../../util/index\";\r\nimport NotificationList from \"@/views/Notifications/NotificationList.vue\";\r\n\r\nexport default {\r\n  name: \"app-toolbar\",\r\n  data: function() {\r\n    return {\r\n      items: [\r\n        {\r\n          icon: \"account_circle\",\r\n          href: \"#\",\r\n          title: \"Perfil\",\r\n          click: (e) => {\r\n            console.log(e);\r\n          },\r\n        },\r\n        {\r\n          icon: \"settings\",\r\n          href: \"#\",\r\n          title: \"ConfiguraciÃ³n\",\r\n          click: (e) => {\r\n            console.log(e);\r\n          },\r\n        },\r\n        {\r\n          icon: \"fullscreen_exit\",\r\n          href: \"#\",\r\n          title: \"Salir\",\r\n          click: this.handleLogout\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    toogleDrawer() {\r\n      this.$store.commit(\"toggleDrawer\");\r\n    },\r\n    handleFullScreen() {\r\n      Util.toggleFullScreen();\r\n    },\r\n    handleLogout() {\r\n      this.$router.push('/login');\r\n    }\r\n  },\r\n  components: {\r\n    NotificationList,\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.span-alert {\r\n  margin-top: 2px !important;\r\n}\r\n</style>"]}]}